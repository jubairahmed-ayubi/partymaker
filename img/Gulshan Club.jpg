<script>
function vendorlogin()
{
	var vendlog_email = document.getElementById('vendlog_email').value.trim();
	var vendlog_password = document.getElementById('vendlog_password').value.trim();
	
	var vndlog_http=false;
	if(navigator.appName==("Micrsoft Internet Explorer")) {	vndlog_http=new ActiveXObject("Microsoft.XMLHTTP"); } else { vndlog_http=new XMLHttpRequest(); }
	
	vndlog_http.open("GET","https://bdwedding.com/ajax-vendor-login.php?vendlog_email="+vendlog_email+"&vendlog_password="+vendlog_password,true);
	vndlog_http.onreadystatechange = function() {		
		if (this.readyState == 4 && this.status == 200) 
		{
			
			if(this.responseText.trim()=='primevendor'){
			window.location='https://bdwedding.com/vendor-membership-plan';
			}
			else{			
			document.getElementById('errshow').innerHTML = this.responseText;
			}			
		}
	}
	vndlog_http.send(null);
}


$(function(){
 $("#vendlogid input").keypress(function (e) {
    if (e.keyCode == 13) {
        vendorlogin();
    }
 });
});

</script>

<script>
