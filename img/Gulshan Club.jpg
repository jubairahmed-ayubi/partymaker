<script>
function subscribe()
{
 	var http_subscri=false;
	if(navigator.appName==("Micrsoft Internet Explorer")) {	http_subscri=new ActiveXObject("Microsoft.XMLHTTP"); } else { http_subscri=new XMLHttpRequest(); }
	
	var subscribers_email=document.getElementById('subscribers_email').value.trim();
	
	if(subscribers_email=='')
	{
		document.getElementById('subscribers_email').value = "";
		document.getElementById("subscribers_email").focus();
	}
	else{
		document.getElementById("subscr_bttn").disabled = true;
		document.getElementById("subscrib_feed").value="Please Wait ...";		
		http_subscri.open("GET","https://bdwedding.com/ajax-subscription.php?subscribers_email="+subscribers_email,true);
		http_subscri.onreadystatechange = function() {
			if (this.readyState == 4 && this.status == 200) 
			{
				document.getElementById('subscrib_feed').innerHTML = this.responseText;
				document.getElementById('subscribers_email').value = "";
				document.getElementById("subscr_bttn").disabled = false;
			}
		}
		http_subscri.send(null);
	}
}
</script>

