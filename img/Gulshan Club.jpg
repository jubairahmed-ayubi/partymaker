<script>
$(document).ready(function(){   

		$(".rating span").mouseenter(function(){
			$('#setratval').html($(this).attr("data-value"));
		});	
		$(".rating").mouseleave(function(){
			var x = $( ".rating span" ).hasClass( "colorbase" );
			if(!x){$('#setratval').html('');} else { 
			var reviews_ratepoint=$('#reviews_ratepoint').val();
			$('#setratval').html(reviews_ratepoint);
			}
		});

    	$(".rating span").click(function(){	
		$('#reviews_ratepoint').val($(this).attr("data-value"));
		var num_pos = $(this).attr("data-sequence");
		$(".rating span").removeClass('colorbase');		
		for(var i=1; i<=num_pos; i++){
			$('#ratpnt_'+i).addClass('colorbase');
		}
		$('#setratval').html($(this).attr("data-value"));	
		
    });
});

function setreview_page(){	
	$('#return_page').val('REVIEW');	
}

function showmoreless(id,status)
{
	if(status=='more'){
		$('#halfcmnt_'+id).hide();
		$('#fullcmnt_'+id).show();
	}
	else{
	$('#fullcmnt_'+id).hide();
	$('#halfcmnt_'+id).show();
	}
}
</script>
