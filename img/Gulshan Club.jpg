<script>
function site_srch(keyword){
	var keyword=keyword.trim();
	var srch_http=false;
	if(navigator.appName==("Micrsoft Internet Explorer")) {	srch_http=new ActiveXObject("Microsoft.XMLHTTP"); } else { srch_http=new XMLHttpRequest(); }
	
	if(keyword!=''){
	document.getElementById("site-srch-list").style.display = "block";
	srch_http.open("GET","https://bdwedding.com/ajax-site-search.php?keyword="+keyword,true);
	srch_http.onreadystatechange = function() {		
		if (this.readyState == 4 && this.status == 200) 
		{
			if(this.responseText.trim()!='NONESRCH')
			{
				document.getElementById('site-srch-list').innerHTML = this.responseText;
			}
			else{
				document.getElementById("site-srch-list").style.display = "none";
			}
		}
	}
	srch_http.send(null);
	}
	else{
		document.getElementById("site-srch-list").style.display = "none";	
	}
}


function gosrch()
{
	var keyword = $('#keyword').val().trim();
	var keyword = keyword.replace('.','^');
	window.location="https://bdwedding.com/search-vendor/"+keyword;
}

$(function(){
 $("#enter-srch input").keypress(function (e) {
    if (e.keyCode == 13) {
        gosrch();
    }
 });
});

</script>
