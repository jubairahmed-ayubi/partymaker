<script>
function getshortlist(vendor_id,user_id)
{
	var shrt_http=false;
	if(navigator.appName==("Micrsoft Internet Explorer")) {	shrt_http=new ActiveXObject("Microsoft.XMLHTTP"); } else { shrt_http=new XMLHttpRequest(); }
	
	shrt_http.open("GET","https://bdwedding.com/ajax-user-shortlist.php?vendor_id="+vendor_id+"&user_id="+user_id,true);
	shrt_http.onreadystatechange = function() {		
		if (this.readyState == 4 && this.status == 200) 
		{
			
			if(this.responseText.trim()=='SHORTLISTED'){				
				$("#srtlst i").addClass("pink");
			}
			else{			
				alert(this.responseText);
			}			
		}
	}
	shrt_http.send(null);
}

function getlikelist(vendor_id,user_id)
{
	var like_http=false;
	if(navigator.appName==("Micrsoft Internet Explorer")) {	like_http=new ActiveXObject("Microsoft.XMLHTTP"); } else { like_http=new XMLHttpRequest(); }
	
	like_http.open("GET","https://bdwedding.com/ajax-user-likelist.php?vendor_id="+vendor_id+"&user_id="+user_id,true);
	like_http.onreadystatechange = function() {		
		if (this.readyState == 4 && this.status == 200) 
		{
			if(this.responseText.trim()=='ErrorHere'){	
				alert('Something wrong, please try again.');			
			}
			else{			
				$("#liklst i").addClass("pink");
				document.getElementById("lovcnt").innerHTML = this.responseText.trim();
				
			}			
		}
	}
	like_http.send(null);
}
</script>

